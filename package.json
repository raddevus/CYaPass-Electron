{
  "name": "CYaPass",
  "version": "1.2.1",
  "description": "Draw your password.\nNever type a password again.\nNever memorize a password again.\nNever make up a password again.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "electron-toolkit": "electron ./node_modules/electron-toolkit",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/raddevus/CYaPass-Electron",
  "keywords": [
    "C'YaPass",
    "CYaPass",
    "password",
    "never",
    "type",
    "again"
  ],
  "author": "roger deutsch",
  "license": "CC0-4.0",
  "devDependencies": {
    "electron": "^16.0.7",
    "electron-builder": "^22.14.5",
    "electron-toolkit": "^1.0.1"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "dot-prop": ">=4.2.1",
    "jquery": "^3.5.1",
    "minimist": "^1.2.6"
  },
  "build": {
    "appId": "com.cyapass.CYaPass",
    "copyright": "Copyright Â© 2019 roger deutsch",
    "productName": "CYaPass",
    "electronVersion": "10.1.1",
    "asar": true,
    "directories": {
      "output": "dist",
      "buildResources": "build"
    },
    "linux": {
      "category": "Utility.Passwords",
      "icon": "CYaPass.png",
      "executableName": "cyapass"
    },
    "win": {
      "icon": "build/CYaPass512.png"
    }
  }
}
